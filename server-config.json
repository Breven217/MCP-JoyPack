{
	"mysql": {
		"name": "mysql",
		"displayName": "Joyful MySQL",
		"description": "A MySQL server for MCP",
		"docsUrl": "https://github.com/breven217/joyfulsql_mcp",
		"mcpConfig": {
			"command": "docker",
			"disabledTools": [],
			"args": [
				"run",
				"--rm",
				"-i",
				"--env-file",
				"~/.mcp/mysql.env",
				"ghcr.io/breven217/joyfulsql_mcp:latest"
			]
		},
		"env": {
			"LOCAL_HOST": "app.bamboolocal.com",
			"LOCAL_USER": "web",
			"LOCAL_PASSWORD": "password",
			"LOCAL_PORT": "3396",
			"ODI_HOST": "127.0.0.1",
			"ODI_USER": "web",
			"ODI_PASSWORD": "",
			"ODI_PORT": "3306",
			"ODI_SSH_PORT": "22",
			"ODI_SSH_KEY": "~/.ssh/id_rsa",
			"WRITE_ACCESS": "false"
		}
	},
	"github": {
		"name": "github",
		"displayName": "GitHub",
		"description": "A GitHub server for MCP",
		"docsUrl": "https://github.com/github/github-mcp-server",
		"mcpConfig": {
			"command": "docker",
			"disabledTools": [],
			"args": [
				"run",
				"-i",
				"--rm",
				"--env-file",
				"~/.mcp/github.env",
				"ghcr.io/github/github-mcp-server"
			]
		},
		"env": {
			"GITHUB_PERSONAL_ACCESS_TOKEN": ""
		}
	},
	"mcp-atlassian": {
		"name": "mcp-atlassian",
		"displayName": "Atlassian",
		"description": "An Atlassian server for MCP",
		"docsUrl": "https://github.com/sooperset/mcp-atlassian",
		"mcpConfig": {
			"command": "docker",
			"disabledTools": [],
			"args": [
				"run",
				"--rm",
				"-i",
				"--env-file",
				"~/.mcp/atlassian.env",
				"ghcr.io/sooperset/mcp-atlassian:latest"
			]
		},
		"env": {
			"JIRA_URL": "https://bamboohr.atlassian.net",
			"JIRA_USERNAME": "[user]@bamboohr.com",
			"JIRA_API_TOKEN": ""
		}
	},
	"rollbar": {
		"name": "rollbar",
		"displayName": "Rollbar",
		"description": "A Rollbar server for MCP",
		"docsUrl": "https://github.com/rollbar/rollbar-mcp-server",
		"mcpConfig": {
			"command": "/Users/bjoyner/.mcp/repos/rollbar-mcp-server/rollbar-wrapper.sh",
			"args": [],
			"env": {}
		},
		"env": {
			"ROLLBAR_ACCESS_TOKEN": ""
		},
		"localSetup": {
			"repo": "https://github.com/rollbar/rollbar-mcp-server.git",
			"command": "node",
			"entryPoint": "build/index.js"
		}
	}
}